<%@ Page Language="VB" AutoEventWireup="false" CodeFile="newProcess.aspx.vb" Inherits="manager_ProcessManager_newProcess" %>

<html xmlns="http://www.w3.org/1999/xhtml" >
<head runat="server">
    <title>新建流程</title>
    <link href="../../App_Themes/StyleSheet.css" type="text/css" rel="Stylesheet"/>
</head>
<body>
    <center>
    <form id="form1" runat="server" style="width:400px">

        <div class="topnav">
            <span class="preload1"></span><span class="preload2"></span>
            <ul id="topnavbar">
                <li class="topmenu"><a class="top_link" id="operater2" href="javascript:void(0);">
                <span>新建流程</span></a></li>
            </ul>
        </div>
        <div class="main" style="border:none;">
            <div class="formtable">
                <table width="100%" cellspacing="0" cellpadding="0" border="0">                    
                    <tr>
                        <th align="right">
                            &nbsp;流程表单：
                        </th>
                        <td width="75%" style="text-align:left">
                            <asp:DropDownList ID="FormList" runat="server" Width="176px" AutoPostBack="True"></asp:DropDownList>
