            sqlstr = "SELECT [KSDM], [KSMC] FROM [GY_KSDM] where "
            For i = 0 To bms.Length - 1
                If i = 0 Then
                    sqlstr = sqlstr & " ksmc = '" & bms(i) & "'"
                Else
                    sqlstr = sqlstr & " or ksmc = '" & bms(i) & "'"
                End If
            Next
        Else
